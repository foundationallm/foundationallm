<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>Ict.PulumiBuildingBlocks</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <OutputType>Library</OutputType>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <AssemblyName>Ict.PulumiBuildingBlocks</AssemblyName>
    <Company>WTW</Company>
    <Product>Ict Pulumi Building Blocks</Product>
    <Description>A collection of building blocks for Pulumi deployments</Description>
    <Copyright>WTW</Copyright>
    <PackageProjectUrl>https://engineering.ict.willistowerswatson.com/docs/engineering/pulumi/Introduction</PackageProjectUrl>
    <PackageReadmeFile></PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Ict.AzureNative" Version="3.1.71" />
    <PackageReference Include="Ict.Configuration" Version="5.0.41" />
    <PackageReference Include="IPNetwork2" Version="2.6.609" />
    <PackageReference Include="Pulumi.AzureNative" Version="2.5.0" />
    <PackageReference Include="Pulumi.Random" Version="4.13.2" />
  </ItemGroup>

</Project>
