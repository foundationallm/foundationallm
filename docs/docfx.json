{
    "build": {
      "globalMetadata": {
        "_appName": "FoundationaLLM",
        "_appTitle": "FoundationaLLM",
        "_appFaviconPath": "media/favicon.ico",
        "_appLogoPath": "media/fllm-icon-50x50.svg",
        "_appFooter": "Â© FoundationaLLM. All rights reserved. | Find out more about FoundationaLLM at <a href=\"https://foundationallm.ai\">foundationallm.ai</a>.",
        "_enableSearch": true,
        "_disableNavbar": false,
        "_disableToc": false,
        "_disableTocFilter": false,
        "_disableSideFilter": false,
        "_disableNewTab": false,
        "_disableContribution": true,
        "_disableBreadcrumb": false,
        "_disableNextArticle": false,
        "_lang": "en"
      },
      "content": [
        {
          "files": [
            "**/*.md",
            "**/*.yml",
            "toc.yml",
            "**/toc.yml"
          ]
        }
      ],
      "resource": [
        {
          "files": [
            "**/media/**"
          ]
        }
      ],
      "output": "_site",
      "globalMetadataFiles": [],
      "fileMetadataFiles": [],
      "template": [
        "default",
        "modern"
      ],
      "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
      "postProcessors": ["ExtractSearchIndex"],
      "markdownEngineName": "markdig",
      "noLangKeyword": false,
      "keepFileLink": false,
      "cleanupCacheHistory": false,
      "disableGitFeatures": false
    }
  }